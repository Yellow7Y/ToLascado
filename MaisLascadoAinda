package Contas_A_Pagar;

public class ContaPagar {
    String descricao;
    Double valor;
   String dataDeVencimento;
   
    public ContaPagar() {
        Fornecedor fornecedor = null;
        String descricao = null;
        double valor = 0;
        String dataVencimento = null;
        
    }

    public ContaPagar(Fornecedor fornecedor, String descricao, double valor, String dataDeVencimento) {
    }

    public void pagar(){
        String descricao = null;
        Double valor = null;
        String dataDeVencimento = null;
        this.descricao = descricao;
        this.valor = valor;
        this.dataDeVencimento = dataDeVencimento;
    }

    public void setDescricao(String descricao) {
    }

    public void setValor(double valor) {
    }

    public void setDataVencimento(String dataDeVencimento) {
    }

    public void setFornecedor(Fornecedor fornecedor) {
    }
}
